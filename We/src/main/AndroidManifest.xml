<?xml version="1.0" encoding="utf-8"?>
<manifest package="com.didikee.weplay"
          xmlns:android="http://schemas.android.com/apk/res/android">

    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE"/>
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE"/>
    <uses-permission android:name="android.permission.READ_CONTACTS"/>
    <uses-permission android:name="android.permission.WRITE_CONTACTS"/>
    <uses-permission android:name="android.permission.READ_PHONE_STATE"/>

    <application
        android:name=".base.WeApp"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/WePink">
        <meta-data
            android:name="AA_DB_NAME"
            android:value="we.db"/>
        <!-- active android 20161011 -->
        <meta-data
            android:name="AA_DB_VERSION"
            android:value="1"/>

        <activity android:name=".ui.WelcomeActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN"/>

                <category android:name="android.intent.category.LAUNCHER"/>
            </intent-filter>
        </activity>
        <activity android:name=".ui.MainActivity">
        </activity>
        <activity android:name=".test.RippleActivity"/>
        <activity android:name=".test.TitleViewActivity">
        </activity>
        <activity android:name=".test.BigSmallActivity">
        </activity>
        <activity android:name=".test.LimitActivity">
        </activity>
        <activity android:name=".test.TestLife2Activity">
        </activity>
        <activity android:name=".test.ActRecyclerViewActivity">
        </activity>
        <activity android:name=".test.TabLayoutActivity">
        </activity>
        <activity android:name=".test.swipe.SwipeLayoutActivity">
        </activity>
    </application>

</manifest>